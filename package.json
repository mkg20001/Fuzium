    "cross-unzip": "^0.2.1",
    "dexie": "^1.5.1",
    "dragula": "^3.7.2",
    "electron-devtools-installer": "^2.2.0",
    "electron-spawn": "^5.0.0",
    "extend": "^3.0.1",
    "extsprintf": "^1.3.0",
    "file-loader": "^0.11.2",
    "file-watcher": "^0.0.2",
    "is-there": "^4.4.2",
    "is-typedarray": "^1.0.0",
    "json-loader": "^0.5.4",
