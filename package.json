{
  "name": "Fuzium",
  "version": "0.0.8",
  "electronVersion": "1.6.2",
  "license": "MIT",
  "description": "Fusion of Electron & Zeronet",
  "author": "Andrew Kidoo",
  "repository": {
    "type": "git",
    "url": "https://github.com/kustomzone/Fuzium"
  },
  "bugs": {
    "url": "https://github.com/kustomzone/Fuzium/issues"
  },
  "main": "main2.js",
  "scripts": {
    "start": "electron main.js",
    "build": "electron-packager . Fuzium --platform win32 --arch x64 --electronVersion=1.6.2 && play done.wav",
    "package": "electron-packager . Fuzium --platform win32 --arch=x64 --electronVersion=1.6.2 && play done.wav",
    "setup": "electron-installer-windows --src dist/app-win32-x64/ --dest dist/installers/",
    "test": "node_modules/.bin/standard --verbose js/**/*.js main/*.js"
  },
  "keywords": [
    "fuzium",
    "zeronet",
    "browser",
    "electron",
    "bittorrent"
  ],
  "devDependencies": {
    "asar": "^0.13.0",
    "babel-preset-react": "6.24.1",
    "electron": "^1.6.11",
    "electron-devtools-installer-offline": "^0.1.4",
    "electron-installer-windows": "^0.2.0",
    "electron-packager": "^8.7.2",
    "electron-reload": "^1.2.1",
    "electron-winstaller": "^2.6.3",
    "grunt": "^1.0.1",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-uglify": "^3.0.1",
    "grunt-electron": "^5.0.0",
    "grunt-electron-installer-windows": "^0.2.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-compass": "2.1.0",
    "standard": "^10.0.2",
    "webpack": "^3.2.0"
  },
  "dependencies": {
